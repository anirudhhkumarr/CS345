\documentclass[a4paper,10pt]{article}
\usepackage{algpseudocode}
\usepackage{algorithm}
%opening

\begin{document}
\section{Solution of 2}
\begin{algorithm}
 \caption{Optimising projects}
 \begin{algorithmic}
  \Function{Optimal}{$H, N$}
    \For{$i \leftarrow$ 1 to $H$}
     \State Optimal[$0$][$i$] $\gets$ 0
    \EndFor
    \For{$i \leftarrow$ 1 to $N$}
     \State Optimal[$i$][$0$] $\gets$ 0
    \EndFor
    \For{$n \leftarrow$ 1 to $H$}
     \For{$h \leftarrow$ 1 to $N$}
      \State Optimal[i]
     \EndFor
    \EndFor
  \EndFunction
 \end{algorithmic}
\end{algorithm}
\end{document}
